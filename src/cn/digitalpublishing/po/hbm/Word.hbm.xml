<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.digitalpublishing.po.Word" table="T_WORD">
		<id name="id" type="java.lang.String">
			<column name="T_ID" length="32" />
			<generator class="uuid.hex"></generator>
		</id>
		<property name="title" type="java.lang.String">
			<column name="T_TITLE" length="128">
				<comment>标题</comment>
			</column>
		</property>
		<property name="name" type="java.lang.String">
			<column name="T_NAME" length="128">
				<comment>word原名</comment>
			</column>
		</property>
		<property name="lastName" type="java.lang.String">
			<column name="T_LASTNAME" length="128">
				<comment>word名</comment>
			</column>
		</property>
		<property name="chainedFile" type="java.lang.String">
			<column name="T_CHAINEDFILE" length="128">
				<comment>word保存</comment>
			</column>
		</property>
		<property name="photo" type="java.lang.String">
			<column name="T_PHOTO" length="128">
				<comment>图片名</comment>
			</column>
		</property>
		<property name="photoLink" type="java.lang.String">
			<column name="T_PHOTOLINK" length="128">
				<comment>图片保存</comment>
			</column>
		</property>
		
	</class>
</hibernate-mapping>